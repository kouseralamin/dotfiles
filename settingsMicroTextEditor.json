{
    "autoclose": false,
    "autoindent": true,
    "autosave": 0,
    "autosu": false,
    "backup": false,
    "basename": false,
    "colorcolumn": 0,
    "colorscheme": "simple",
    "comment": true,
    "cursorline": true,
    "diff": false,
    "diffgutter": false,
    "encoding": "utf-8",
    "eofnewline": false,
    "fastdirty": false,
    "fileformat": "unix",
    "filetype": "unknown",
    "ftoptions": true,
    "hidehelp": true,
    "ignorecase": false,
    "indentchar": " ",
    "infobar": false,
    "keepautoindent": false,
    "keymenu": false,
    "linter": false,
    "literate": false,
    "matchbrace": true,
    "matchbraceleft": true,
    "mkparents": false,
    "mouse": true,
    "paste": true,
    "pluginchannels": [
        "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"
    ],
    "pluginrepos": [],
    "readonly": false,
    "rmtrailingws": false,
    "ruler": true,
    "savecursor": false,
    "savehistory": false,
    "saveundo": false,
    "scrollbar": false,
    "scrollmargin": 3,
    "scrollspeed": 1,
    "smartpaste": false,
    "softwrap": false,
    "splitbottom": false,
    "splitright": false,
    "status": false,
    "statusformatl": "$(filename) $(modified)($(line),$(col)) $(status.paste)| ft:$(opt:filetype) | $(opt:fileformat) | $(opt:encoding)",
    "statusformatr": "$(bind:ToggleKeyMenu): bindings, $(bind:ToggleHelp): help",
    "statusline": false,
    "sucmd": "sudo",
    "syntax": true,
    "tabmovement": false,
    "tabsize": 4,
    "tabstospaces": false,
    "termtitle": false,
    "useprimary": true,
    "xterm": false
}